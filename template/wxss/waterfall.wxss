/* 下拉店铺 */

.store_item {
  padding-top: 20rpx;
}
.store_item image{
  width: 224rpx;
  height: 208rpx;
}
.store_info{
  width: 468rpx;
  padding-left: 20rpx;
  font-size: 24rpx;
  color: #737373;
  line-height: 1.6;
}
.textover{
  display:block;
  width: 280rpx;
  height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  white-space: nowrap;
}
.store_title {
  font-size:18rpx;
  margin-bottom:10rpx;
}
.store_title text{
  font-size: 30rpx;
  color: #111111;
  font-weight: bold;
  margin-right: 16rpx;
}
.pinkB{
  color:#FF1D3A;
  font-size: 30rpx;
  font-weight: bold;
  margin-right: 4rpx;
}
.pink{
  color:#FF1D3A;
  font-weight: bold;
}
.smallsize{
  font-size: 16rpx;
  margin-right: 36rpx;
}
.formal{
  text-decoration: line-through;
}
.marks text{
  font-size: 20rpx;
  padding: 0 6rpx;
  border: 2rpx solid #FF2245;
  border-radius: 6rpx;
  margin-right: 6rpx;
  color:#FF2245;
}


.loading {
  background: url('data:image/svg+xml;base64,') no-repeat;
  background-size: cover;
  width: 50rpx;
  height: 50rpx;
  -webkit-animation: loading 1s steps(12, end) infinite;
  animation: loading 1s steps(12, end) infinite;
}

@-webkit-keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

@keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
    transform: rotate3d(0, 0, 1, 360deg);
  }
}


.address,.money{
  justify-content: space-between;
}