/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  overflow-y: scroll;
} 
.scrollBox{
  overflow-y: scroll;
}
/* 隐藏滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;

}
/*flexbox盒模型  */
.flexbox{
  display: flex;
  flex-direction: row;
}
/* 圆角背景盒子 */
.radiusbox {
  width: 662rpx;
  padding: 0 20rpx 20rpx;
  margin: 0 auto 34rpx;
  background-color: #fff;
  border-radius: 15rpx;

}
/* 下边框为0 */
.nobottomB{
  border-bottom:0;
}

.ellipsis{
  overflow: hidden;
  text-overflow: ellipsis;
  -ms-text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  white-space: nowrap;
}
.navigator-hover {
  background-color:transparent;

}
